curl 'http://localhost:8080/word/' \
-H 'Accept-Encoding: gzip, deflate, sdch' \
-H 'Accept-Language: zh-CN,zh;q=0.8,en-US;q=0.6,en;q=0.4' \
-H 'Upgrade-Insecure-Requests: 1' \
-H 'User-Agent: Mozilla/5.0 (Windows NT 10.0; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/53.0.2779.1 Safari/537.36' \
-H 'Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8' \
-H 'Cache-Control: max-age=0' \
-H 'Connection: keep-alive' \
-H 'Content-Type: application/json; charset=UTF-8' \
--data-ascii '{"word": "Liu Yuhui"}' \
--compressed 